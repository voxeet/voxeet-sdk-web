{
  "name": "@voxeet/voxeet-web-sdk",
  "version": "3.10.0",
  "description": "The Voxeet Javascript SDK",
  "author": "Thomas Gourgues (tgourgues@voxeet.com)",
  "main": "dist/voxeet-sdk.js",
  "license": "LicenseRef-LICENSE",
  "browserslist": [
    "IE >= 11",
    "Chrome >= 58"
  ],
  "browser": {
    "ws": false,
    "buffer": false
  },
  "scripts": {
    "audit": "yarn audit",
    "generate-licenses": "yarn licenses generate-disclaimer --prod --non-interactive --no-progress -s voxeet > dist/THIRD_PARTY_LICENSE.md",
    "check": "prettier -w --check  src",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "doc": "typedoc --excludeTags asmemberof --disableSources --entryPointStrategy expand src",
    "clean": "rimraf dist types coverage",
    "clean-types": "rimraf types/utils types/api types/packages types/lib types/events/EventBus.d.ts types/events/EventFactory.d.ts types/events/EventBusInstance.d.ts types/models/Telemetry.d.ts types/services/browserevent types/services/conference/ConferenceManager.d.ts types/services/session/SessionClient.d.ts types/services/telemetry types/services/videofilters/VideoFiltersHelper.d.ts types/services/videofilters/VideoFilterInConferenceController.d.ts",
    "build": "npm run check && npm run audit && webpack --progress",
    "postbuild": "npm run clean-types && node clean-ssr.js",
    "watch": "webpack --progress --watch",
    "watch:prod": "cross-env NODE_ENV=production npm run watch",
    "watch:client": "cross-env NODE_ENV=staging CLIENT=true npm run watch",
    "build:client": "cross-env NODE_ENV=staging CLIENT=true npm run build",
    "build:testapp": "npm run build -- --config webpack.test.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:staging": "cross-env NODE_ENV=staging npm run build",
    "build:development": "cross-env NODE_ENV=development npm run build",
    "build:devssl": "cross-env NODE_ENV=devssl npm run build",
    "get-version": "echo $npm_package_version",
    "start": "webpack-dev-server --hot --content-base samples/",
    "start:testapp": "cross-env NODE_ENV=staging LOCALVERSION=$(npm run --silent get-version)-staging webpack-dev-server --config webpack.test.js --hot",
    "start:testapp:prod": "cross-env NODE_ENV=production LOCALVERSION=$(npm run --silent get-version)-production webpack-dev-server --config webpack.test.js --hot",
    "start:testapp:devssl": "cross-env NODE_ENV=devssl LOCALVERSION=$(npm run --silent get-version)-devssl webpack-dev-server --config webpack.test.js --hot",
    "start:testapp:development": "cross-env NODE_ENV=development LOCALVERSION=$(npm run --silent get-version)-development webpack-dev-server --config webpack.test.js --hot",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "start:devssl": "cross-env NODE_ENV=devssl npm run start",
    "prepublishOnly": "npm run clean && npm run build && npm run generate-licenses",
    "lint": "eslint src; exit 0",
    "jest:ci": "jest --ci --reporters=default --reporters=jest-junit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voxeet/voxeet-sdk-web.git"
  },
  "devDependencies": {
    "@dolby-dvc/dvwc": "1.4.4",
    "@dolbyio/vsl": "1.0.0-develop-4123afa7-682112",
    "@millicast/sdk": "^0.1.36",
    "@size-limit/file": "^8.1.0",
    "@types/bowser": "^1.1.1",
    "@types/debug": "^4.1.4",
    "@types/express": "4.17.2",
    "@types/express-serve-static-core": "4.17.21",
    "@types/jest": "^27.5.1",
    "@types/lodash.isequal": "^4.5.6",
    "@types/md5": "^2.3.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.2",
    "browserslist": "^4.5.2",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^6.4.1",
    "core-js": "^3.26.1",
    "cross-env": "^2.0.1",
    "crypto-browserify": "^3.12.0",
    "csv": "^5.5.3",
    "docdash": "^0.3.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "husky": "4.3.0",
    "jest": "27.5.1",
    "jest-extended": "^3.2.0",
    "jest-html-reporters": "^2.1.0",
    "jest-junit": "^12.0.0",
    "jest-mock-extended": "^2.0.7",
    "loglevel": "^1.8.1",
    "md5": "^2.3.0",
    "moment-locales-webpack-plugin": "1.2.0",
    "preprocessify": "^1.0.1",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.0",
    "rimraf": "^2.4.3",
    "size-limit": "^8.1.0",
    "source-map-loader": "1.1.2",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^0.13.0",
    "tough-cookie": "^4.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^6.0.0",
    "typedoc": "0.22.18",
    "typedoc-custom-theme": "file:docs/typedoc-custom-theme",
    "typescript": "4.0.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^1.6.0",
    "worker-url": "^1.1.0",
    "worklet-loader": "^2.0.0",
    "yargs": "^3.17.1"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "bowser": "^2.8.1",
    "browserslist": "^4.5.2",
    "btoa": "^1.1.2",
    "debug": "^4.3.1",
    "events": "^1.1.0",
    "get-float-time-domain-data": "^0.1.0",
    "global": "^4.4.0",
    "jwt-decode": "3.1.2",
    "lodash.isequal": "4.5.0",
    "lodash.merge": "4.6.2",
    "moment": "^2.29.4",
    "sdp": "^2.3.0",
    "webrtc-adapter": "^6.3.2"
  },
  "files": [
    "README.md",
    "dist/",
    "types/"
  ],
  "types": "types/VoxeetSDK.d.ts"
}
